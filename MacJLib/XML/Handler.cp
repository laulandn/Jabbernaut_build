g &inName){mDepth--;for (int N=0; N<mDepth; N++) cout << '\t';std::cout << "</" << inName << ">" << endl;}void CTestHandler::HandleCharacterData(const std::string &inData){for (int N=0; N<mDepth; N++) cout << '\t';std::cout << inData << endl;}#if USE_MACJLIB_NS}#endif