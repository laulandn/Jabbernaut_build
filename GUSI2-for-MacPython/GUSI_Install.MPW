######################################################################## 	$Id: GUSI_Install.MPW,v 1.1 1999/06/30 07:42:00 neeri Exp $##	Project	:	GUSI 2				-	Grand Unified Socket Interface#	File	:	GUSI_Install.MPW	-	Install for MPW environment#	Author	:	Matthias Neeracher		Language	:	MPW#	#	$Log: GUSI_Install.MPW,v $#	Revision 1.1  1999/06/30 07:42:00  neeri#	Getting ready to release 2.0b3#	Echo '### '`date -t`' ----- Begin GUSI Installation'             ########################################### Install our path in startup items#Set Exit 0Set D `Directory`If `Evaluate "{0}" =~ /(:)1/`	Directory "{1}"EndSet GUSI "`Directory`"Directory "{D}"If `Exists "{PrefsFolder}Startup Items:"` == "" 	Set StartupItems "{MPW}Startup Items:"Else	Set StartupItems "{PrefsFolder}Startup Items:"	EndSet UserStartupGUSI "{StartupItems}UserStartup쩏USI"If `Exists "{UserStartupGUSI}"` == ""	Echo '### '`date -t`' ----- Creating "'"{UserStartupGUSI}"'"'	Begin		Echo '# UserStartup쩏USI - Paths for GUSI and friends'		Echo 		Echo '# GUSI             Grand Unified Socket Library'		Echo 'Set -e GUSI        "xxx"'		Echo 		Echo '# STLport          Portable STL implementation'		Echo 'Set -e STLport     "{GUSI}:STLport:"'		Echo 		Echo '# SFIO             Safe/Fast IO Package'		Echo 'Set -e SFIO        "{GUSI}:sfio:"'	End> "{UserStartupGUSI}"Else	Echo '### '`date -t`' ----- Editing "'"{UserStartupGUSI}"'"'EndEcho '### '`date -t`' ----- Configuring GUSI Path as "'{GUSI}'"'Rename "{UserStartupGUSI}" "{UserStartupGUSI}".oldStreamEdit -e '/Set -e GUSI/ Replace /"킷"/ ""'{GUSI}'""'  "{UserStartupGUSI}".old>"{UserStartupGUSI}"Delete -y "{UserStartupGUSI}".old             ########################################### Copy MPW standard header#Echo '### '`date -t`' ----- Installing "'"{Cincludes}errno.h"'" as mpw/errno.h'Begin	NewFolder "{GUSI}"include:mpw:  && Duplicate -y "{Cincludes}errno.h" "{GUSI}"include:mpw:errno.hEnd  Dev:Null             #########################################	Echo '### '`date -t`' ----- End GUSI Installation'