########################################################################	Project	:	GUSI				-	Grand Unified Socket Interface#	File		:	Makefile			-	Build examples#	Author	:	Matthias Neeracher#	Language	:	MPW Shell/Make#	#	$Log: Makefile,v $#	Revision 1.1  1999/08/26 05:47:19  neeri#	Added makefile for MPW variants#	#######################################################################SFIOInc		=	-i {SFIO}include:MPWInc		= 	-i ::include -i "{CIncludes}" -i ::DCon:Headers:  -i {STLport}stl:COpt			=	-d SystemSevenOrLater -d OLDROUTINENAMES=0 -sym on CPOpt			=  -bool on -EROpt68K		=	-model far -mc68020 -mbg onOptPPC		=	-tracebackMPWOpt		=	{COpt} {MPWInc} -includes unix -w 2,35CSC			=	SC {MPWOpt} {Opt68K}CPSC			=	SCpp {MPWOpt} {Opt68K} {CPOpt}CMRC			=	MrC {SFIOInc} {MPWOpt} {OptPPC}CPMRC			=	MrCpp {SFIOInc} {MPWOpt} {OptPPC} {CPOpt} LOpt			= 	-sym on -w MPWLOpt		=	{LOpt} -c '????' -t APPLLinkSC		=	Link {MPWLOpt}  -model far -srtsg all -br 020LinkMrC		= 	PPCLink {MPWLOpt}LibFilesSC	=		"{Libraries}SIOW.far.o"								"{CLibraries}CPlusLib.o"							"{CLibraries}StdCLib.o"								"{Libraries}MacRuntime.o"							"{Libraries}Interface.o"							"{Libraries}IntEnv.o"								"{Libraries}MathLib.o"								"{Libraries}ToolLibs.o"								"{CLibraries}IOStreams.far.o"						"{Libraries}OpenTransport.o"						"{Libraries}OpenTransportApp.o"					"{Libraries}OpenTptInet.o"LibFilesMrC	= 		"{SFIO}lib:Sfio.MrC.Lib"							"{PPCLIbraries}PPCSIOW.o"							"{PPCLibraries}MrCPlusLib.o"						"{PPCLibraries}PPCStdCLib.o"						"{PPCLibraries}StdCRuntime.o"						"{PPCLibraries}PPCCRuntime.o"						"{PPCLibraries}PowerMathLib"						"{PPCLibraries}PPCToolLibs.o"						"{SharedLibraries}InterfaceLib"					"{SharedLibraries}ThreadsLib"						"{PPCLibraries}MrCIOStreams.o"					"{SharedLibraries}StdCLib"							"{SharedLibraries}OpenTransportLib"				"{SharedLibraries}OpenTptInternetLib"			"{PPCLibraries}OpenTransportAppPPC.o"			"{PPCLibraries}OpenTptInetPPC.o".c.o.SC		.c	{CSC} {Default}.c -o {Default}.c.o.SC.c.o.MrC		.c	{CMRC} {Default}.c -o {Default}.c.o.MrCGUSIConfig_MTINET.cp.o.SC		::test:GUSIConfig_MTINET.cp	{CPSC} ::test:GUSIConfig_MTINET.cp -o GUSIConfig_MTINET.cp.o.SCGUSIConfig_MTINET.cp.o.MrC		::test:GUSIConfig_MTINET.cp	{CPMRC} ::test:GUSIConfig_MTINET.cp -o GUSIConfig_MTINET.cp.o.MrCall  GUSIFileTest.SC GUSIFileTest.MrCGUSIFileTest.SC  GUSITest.c.o.SC GUSIFileTest.c.o.SC GUSIConfig_MTINET.cp.o.SC	{LinkSC} -o GUSIFileTest.SC GUSITest.c.o.SC GUSIFileTest.c.o.SC GUSIConfig_MTINET.cp.o.SC 		::lib:GUSI_SIOW.SC.Lib ::lib:GUSI_Stdio.SC.Lib ::lib:GUSI_Core.SC.Lib 		{LibFilesSC}	Rez -a -o GUSIFileTest.SC "{RIncludes}"SIOW.rGUSIFileTest.MrC  GUSITest.c.o.MrC GUSIFileTest.c.o.MrC GUSIConfig_MTINET.cp.o.MrC	{LinkMrC} -o GUSIFileTest.MrC GUSITest.c.o.MrC GUSIFileTest.c.o.MrC GUSIConfig_MTINET.cp.o.MrC 		::lib:GUSI_SIOW.MrC.Lib ::lib:GUSI_Sfio.MrC.Lib ::lib:GUSI_Core.MrC.Lib 		{LibFilesMrC}	Rez -a -o GUSIFileTest.MrC "{RIncludes}"SIOW.r