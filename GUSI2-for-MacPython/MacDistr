perl -Sx "{0}" {"Parameters"}; Exit {Status}#!perl$vers = "219";%SUFFIX = ();use ExtUtils::Manifest;($action,$distr) = @ARGV;$ExtUtils::Manifest::MANIFEST = "MANIFEST.$distr" if $distr;$DistName = 	"::Distr:GUSI_${vers}" . ($distr ? ($SUFFIX{$distr} || "_$distr") : "");if ($action eq "check") {	ExtUtils::Manifest::fullcheck();} elsif ($action eq "copy") {	ExtUtils::Manifest::manicopy(ExtUtils::Manifest::maniread(), $DistName, "cp");} elsif ($action eq "make") {	ExtUtils::Manifest::mkmanifest();} elsif ($action eq "name") {	print "$DistName\n";} else {	die "Usage is: MacDistr (check|copy|make|name) ()";}